views {

  view customer of customer {
    include *, saas.ui

    style customer {
      color sky
    }
  }

  view saas of saas {
    title 'Our Saas - Overview'
    include *

    style saas.* {
      color sky
    }
  }

  view ui of saas.ui {
    include *

    style saas.ui {
      color sky
    }
  }

  view backend of saas.backend {
    include *
    style saas.backend {
      color sky
    }
  }

}
