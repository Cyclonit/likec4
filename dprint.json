{
  "lineWidth": 120,
  "useTabs": false,
  "typescript": {
    "semiColons": "asi",
    "trailingCommas": "never",
    "importDeclaration.preferSingleLine": true,
    "quoteStyle": "alwaysSingle",
    "jsxOpeningElement.bracketPosition": "maintain",
    "jsx.quoteStyle": "preferDouble",
    "jsxSelfClosingElement.bracketPosition": "maintain",
    "jsx.bracketPosition": "maintain"
  },
  "json": {
  },
  "prettier": {
    "proseWrap": "preserve",
    "singleQuote": true,
    "semi": false,
    "embeddedLanguageFormatting": "auto",
    "trailingComma": "none",
    "bracketSpacing": true,
    "quoteProps": "consistent"
  },
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    ".yarn/*",
    ".turbo",
    ".vscode",
    ".vscode-test-web",
    ".next",
    "docs/tsconfig.*",
    "docs/out",
    "docs/components/ui/",
    "examples/*/dist",
    "packages/*/dist",
    "packages/*/contrib",
    "packages/*/src/generated"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.90.1.wasm",
    "https://plugins.dprint.dev/json-0.19.2.wasm",
    "https://plugins.dprint.dev/markdown-0.16.4.wasm",
    "https://plugins.dprint.dev/prettier-0.39.0.json@896b70f29ef8213c1b0ba81a93cee9c2d4f39ac2194040313cd433906db7bc7c"
  ]
}
