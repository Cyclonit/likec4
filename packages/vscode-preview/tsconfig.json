{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "outDir": "dist",
    "declarationDir": "dist",
    "tsBuildInfoFile": ".tsbuildinfo/src.tsbuildinfo",
    "noPropertyAccessFromIndexSignature": false,
    "noUnusedParameters": false,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "vite/client",
      "vscode-webview"
    ],
    "jsx": "react-jsxdev"
  },
  "include": [
    "protocol.ts",
    "./src"
  ],
  "exclude": [
    "dist"
  ],
  "references": [
    {
      "path": "../core"
    },
    {
      "path": "../diagram"
    }
  ]
}
