model {

  premium = actor 'Premium Customer' {
    description '
      The customer of Cloud system
      with Premium subscription
    '
    style {
      color red
    }
  }
  premium -> cloud.ui.dashboard
}

views {
  view premium of premium {
    title: 'Premium Customer Use-cases'
    include
      *
      ,cloud.ui
      ,premium -> cloud.ui.*
  }
}
