{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "../.tsbuildinfo/app.tsbuildinfo",
    "target": "ES2022",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "esnext",
    "outDir": "../dist/__app__",
    "esModuleInterop": true,
    "noPropertyAccessFromIndexSignature": false,
    "noUnusedParameters": false,
    "allowJs": true,
    "checkJs": true,
    "jsx": "react-jsx",
    "rootDir": ".",
    "types": ["vite/client"]
  },
  "include": [
    "./src",
    "./webcomponent",
    "./react"
  ],
  "references": [
    {
      "path": "../../core"
    },
    {
      "path": "../../diagram"
    },
    {
      "path": "../../diagrams"
    }
  ]
}
