{
  "name": "@likec4/diagrams",
  "version": "0.6.3",
  "license": "MIT",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://like-c4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git",
    "directory": "packages/diagrams"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./embedded": {
      "types": "./src/embedded/index.ts",
      "import": "./dist/embedded.js",
      "require": "./dist/embedded.cjs"
    },
    "./browser": {
      "types": "./src/browser/index.ts",
      "import": "./dist/browser.js",
      "require": "./dist/browser.cjs"
    },
    "./src/*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ],
    "./dist/style.css": "./dist/style.css"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "compile": "tsc --emitDeclarationOnly",
    "build:release": "run-s 'build:ts' 'build:tsd'",
    "build:ts": "yarn vite build --mode=production",
    "build:tsd": "tsc -p tsconfig.build.json",
    "build": "run-s 'build:ts' 'build:tsd'",
    "dev:ts": "yarn vite build --watch",
    "dev:tsd": "run build:tsd --watch",
    "dev": "run-p 'dev:*'",
    "lint": "run -T eslint src/ --fix",
    "clean": "run -T rimraf dist"
  },
  "dependencies": {
    "@react-spring/konva": "^9.7.2",
    "konva": "^9.0.0",
    "react-konva": "^18.2.7"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@likec4/core": "workspace:*",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-dialog": "^1.0.3",
    "@react-hookz/deep-equal": "^1.0.4",
    "@react-hookz/web": "^23.0.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@vanilla-extract/css": "^1.11.0",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.4.0",
    "@vanilla-extract/vite-plugin": "^3.8.0",
    "clsx": "^1.2.1",
    "khroma": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "polished": "^4.2.2",
    "rambdax": "^9.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tilg": "^0.1.1",
    "tiny-invariant": "^1.3.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.2"
  }
}
