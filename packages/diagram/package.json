  "type": "module",
  "sideEffects": false,
  "exports": {
    "./bundle": {
      "types": "./dist/bundle.d.ts",
      "default": "./bundle/index.js"
    },
    "./bundle/style.css": "./bundle/style.css",
    ".": "./src/index.ts"
  },
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build:turbo": "run -T turbo build --filter='diagram'",
    "build": "unbuild",
    "prepack": "unbuild",
    "lint": "run -T eslint src/ --fix",
    "clean": "run -T rimraf dist bundle"
  },
  "dependencies": {
    "@likec4/core": "workspace:*"
  },
  "peerDependencies": {
    "@mantine/core": "^7.6.2",
