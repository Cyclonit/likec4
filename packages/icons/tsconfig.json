{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "lib": [
      "es2020",
      "dom"
    ],
    "strict": false,
    "target": "ES2020",
    "module": "Preserve",
    "allowArbitraryExtensions": true,
    "assumeChangesOnlyAffectDirectDependencies": true,
    "isolatedModules": true,
    "declaration": true,
    "skipLibCheck": true,
    "allowJs": false,
    "checkJs": false,
    "jsx": "react-jsx"
  },
  "files": [
    "all.tsx"
  ]
}
