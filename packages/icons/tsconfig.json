{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "lib": [
      "es2020",
      "dom"
    ],
    "noEmit": true,
    "strict": false,
    "target": "ES2022",
    "module": "ES2022",
    "isolatedModules": true,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": false,
    "jsx": "react-jsx",
    "types": [
      "react",
      "react-dom"
    ],
  },
  "include": [
    ".",
  ],
  "exclude": [
    ".tmp/",
    "scripts/"
  ]
}
