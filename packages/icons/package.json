{
  "name": "@likec4/icons",
  "version": "1.5.0",
  "license": "MIT",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git",
    "directory": "packages/icons"
  },
  "bugs": "https://github.com/likec4/likec4/issues",
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/tech/index.ts",
    "./all": "./src/all.tsx",
    "./aws": "./src/aws/index.ts",
    "./aws/*": "./src/aws/*.tsx",
    "./tech": "./src/tech/index.ts",
    "./tech/*": "./src/tech/*.tsx",
    "./gcp": "./src/gcp/index.ts",
    "./gcp/*": "./src/gcp/*.tsx",
    "./*": "./src/tech/*.tsx"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/tech/index.d.ts",
        "default": "./dist/tech/index.js"
      },
      "./all": {
        "types": "./dist/all.d.ts",
        "default": "./dist/all.jsx"
      },
      "./aws": {
        "types": "./dist/aws/index.d.ts",
        "default": "./dist/aws/index.js"
      },
      "./aws/*": {
        "types": "./dist/aws/*.d.ts",
        "default": "./dist/aws/*.jsx"
      },
      "./tech": {
        "types": "./dist/tech/index.d.ts",
        "default": "./dist/tech/index.js"
      },
      "./tech/*": {
        "types": "./dist/tech/*.d.ts",
        "default": "./dist/tech/*.jsx"
      },
      "./gcp": {
        "types": "./dist/gcp/index.d.ts",
        "default": "./dist/gcp/index.js"
      },
      "./gcp/*": {
        "types": "./dist/gcp/*.d.ts",
        "default": "./dist/gcp/*.jsx"
      },
      "./*": {
        "types": "./dist/tech/*.d.ts",
        "default": "./dist/tech/*.jsx"
      }
    }
  },
  "scripts": {
    "update-icons": "node scripts/update-icons.mjs",
    "prepack": "tsc",
    "generate": "node scripts/generate.mjs"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "consola": "^3.2.3",
    "execa": "^9.3.0",
    "glob": "^11.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.3"
  },
  "packageManager": "yarn@4.3.1"
}
