---
import { LikeC4View as View, type LikeC4ViewId } from './likec4-views'
interface Props {
  viewId: LikeC4ViewId;
}
const { viewId } = Astro.props
---

<View viewId={viewId} className="likec4-bigbank-view" client:only="react" injectFontCss={false} />

<style>
  .likec4-bigbank-view {
    max-width: 800px;
    margin-left: auto;
    margin-right: auto;
  }
</style>
