{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "generate": {
      "dependsOn": [
        "^generate"
      ],
      "outputs": [
        "src/routeTree.gen.ts",
        "src/icons.gen.tsx"
      ],
      "inputs": [
        "scripts/*",
        "src/routes/*.tsx",
        "tsr.config.json",
        "package.json"
      ]
    },
    "build": {
      "dependsOn": [
        "generate"
      ],
      "outputs": [
        "dist/**"
      ]
    }
  }
}
