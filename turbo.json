{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "generate": {},
    "typecheck": {
      "cache": false,
      "dependsOn": ["generate", "^build"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["generate", "^build"],
      "outputs": ["dist/**", "tsconfig.tsbuildinfo"]
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "test": {
      "cache": false,
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    }
  },
  "globalDependencies": ["packages/tsconfig/*", "yarn.lock"]
}
