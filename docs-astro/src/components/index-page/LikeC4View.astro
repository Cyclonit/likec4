---
import { LikeC4View as C4View, LikeC4ViewId } from './likec4-views.jsx'
interface Props {
  viewId: LikeC4ViewId;
}
const { viewId } = Astro.props
---

<C4View viewId={viewId} className="likec4-index-view" client:only="react" />

<style>
  .likec4-index-view {
    max-width: 700px;
    margin-left: auto;
    margin-right: auto;
  }
</style>
